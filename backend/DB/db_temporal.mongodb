db.createCollection('infraestructura', {
    validator: {
        $jsonSchema: {
            bsonType: "object",
            required: [
                "id",
                'tipo',
            ],
            properties: {
                id: {
                    bsonType: "int",
                    description: "zone indentify must be integer type"
                },
                cc_encargado: {
                    bsonType: "number",
                    description: "encargado must be string type"
                },
                tipo: {
                    bsonType: "string",
                    enum: [
                        'Departamento',
                        'Area',
                        'Habitat'
                    ],
                    description: "place name must be string type"
                },
                descripcion: {
                    bsonType: "string",
                    description: "description place must be string type"
                },
                dimensiones: {
                    bsonType: "string",
                    description: 'dimensiones de la estructura'
                }
            }
        }
    }
});
