    { _id: "historial_mantenimiento_id", sequence_value: 0 },
    { _id: "historial_medico_id", sequence_value: 0 },
    { _id: "eventos_id", sequence_value: 0 },
    { _id: "finanzas_id", sequence_value: 0 },
    { _id: "organizaciones_id", sequence_value: 0 },
    { _id: "animales_id", sequence_value: 0 },
]);

